@model IEnumerable<TheWayFreeClinicVMS.Models.Econtact>
@if (!Model.Any())
{
    @: None
}
else
{
foreach (var item in Model)
{
    @Html.DisplayFor(modelItem => item.ecLastName)
    @: 
    @Html.DisplayFor(modelItem => item.ecFirstName) <br />
    @:Phone: 
    @Html.DisplayFor(modelItem => item.ecPhone);
}
}